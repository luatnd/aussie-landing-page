{
  "private": true,
  "name": "aussie-land",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "build": "export AUSSIE_SOURCE_MAP=false && preact build",
    "build-map": "export AUSSIE_SOURCE_MAP=true && preact build",
    "serve": "yarn build-map && serve -s build",
    "dev": "export AUSSIE_SOURCE_MAP=true && preact watch",
    "lint": "eslint src"
  },
  "dependencies": {
    "preact": "^8.2.1",
    "preact-compat": "^3.17.0",
    "preact-router": "^2.5.5"
  },
  "devDependencies": {
    "eslint": "^4.5.0",
    "eslint-config-synacor": "^1.1.0",
    "if-env": "^1.0.0",
    "node-sass": "^4.7.2",
    "preact-cli": "^2.0.0",
    "sass-loader": "^6.0.6"
  },
  "browserslist": [
    "> 1%",
    "IE >= 9",
    "last 2 versions"
  ],
  "eslintConfig": {
    "extends": "eslint-config-synacor"
  },
  "eslintIgnore": [
    "build/*"
  ]
}
